{
  "version": 2,
  "functions": {
    "cmd/url-shortener/main.go": {
      "runtime": "@vercel/go@1.0.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "cmd/url-shortener/main.go"
    }
  ],
  "env": {
    "CONFIG_PATH": "/root/apps/url-shortener/config/prod.yaml",
    "DATABASE_URL": "file:./storage.db"
  }
}